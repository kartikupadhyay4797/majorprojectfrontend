<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.majorprojectfrontend">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CartActivity"></activity>
        <activity android:name=".ItemStockList"
            android:label="List of Items"/>
        <activity android:name=".DealerList"
            android:label="List of Dealers"/>
        <activity
            android:name=".ShowImage"
            android:theme="@style/Theme.Transparent" />
        <activity android:name=".OrderList"
            android:label="Order History"/>
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/AppFullScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>